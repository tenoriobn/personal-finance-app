generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Budget {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  name      String
  limit     Float
  createdAt DateTime @default(now())
}